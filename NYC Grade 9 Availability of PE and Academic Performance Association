    "# Calculating the sum of ELA and Math scores\n",
    "\n",
    "# Convert scores from string to floats\n",
    "df_test_score['Mean Scale Score-ELA'] = df_test_score['Mean Scale Score-ELA'].astype('float')\n",
    "df_test_score['Mean Scale Score-Math'] = df_test_score['Mean Scale Score-Math'].astype('float')\n",
    "\n",
    "# Add the scores\n",
    "df_test_score['Mean Scale Score-Sum'] = df_test_score['Mean Scale Score-ELA'] + df_test_score['Mean Scale Score-Math']\n",
    "df_test_score.head()\n"
   ]

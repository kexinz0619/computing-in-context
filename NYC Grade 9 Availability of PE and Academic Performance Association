   "source": [
    "# Testing multiple regression assumtions\n",
    "# 2. Equal variance \n",
    "pred_val = regression.fittedvalues.copy()\n",
    "true_val = A.values.copy()\n",
    "residual = true_val - pred_val\n",
    "fig, ax = plt.subplots()\n",
    "_ = ax.scatter(residual, pred_val)\n",
    "\n",
    "# Use Harvey-Collier multiplier test, for linearity\n",
    "import statsmodels.stats.api as sm\n",
    "sm.linear_harvey_collier(regression)\n",
    "\n",
    "# p > 0.05, there is a linear relationship\n"
   ]
